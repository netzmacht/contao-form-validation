{
  "name":"netzmacht/contao-form-validation",
  "description":"Javascript form validation.",
  "keywords":["contao", "form", "validation", "frontend", "javascript"],
  "type":"contao-module",
  "license":"LGPL-3.0+",
  "authors":[
    {
      "name":"David Molineus",
      "email":"mail@netzmacht.de",
      "homepage":"http://www.netzmacht.de",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"mail@netzmacht.de",
